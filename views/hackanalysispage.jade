  body(onload="func()")
    #container
      #header
        #logo
          img(src='https://raw.githubusercontent.com/Shinseulgi/Hanium/master/webla.jpg')               
        #up_menu
          ul
            li
              a(href='/analysis') Log Analysis     
            li
              a.active(href='/hack_analysis') Hacking Analysis       
            li
              a(href='/pre_analysis') History 
      #medium           
        #userinfo 
          br
          | Welcome -----
          .divshowid
           div 안녕하세요! #{id}님!
          br
          a(href='#') Setting | &nbsp;&nbsp;
          a(href='#') Logout
          br
          br
          br
      #down
        #sideinfo
          span
            a(id='hackanalysis',href='/hackanalysis') Injection(#{injcnt})  
          span
            a(id='hackanalysis_basm',href='/hackanalysis_basm') Broken Authentication and Session Management(#{basmcnt}) 
          span
            a(id='hackanalysis_xss',href='/hackanalysis_xss') Cross Site Scripting(#{xsscnt}) 
          span
            a(id='hackanalysis_sde',href='/hackanalysis_sde') Sensitive Data Exposure(#{sdecnt})  
        #content
          .div4label
            | Log Information
          table.outtable
            tr
              td
                table.center
                  tr(style='color:white;background-color:grey')
                    th(width="8%") date
                    th(width="6%") time
                    th(width="12%") srcIP
                    th(width="12%") dstIP
                    th(width="4%") proto
                    th(width="8%") csMethod
                    th(width="30%") page
                    th param
                    th(width="6%") status
            tr
              td
                .outcenter
                  table.center(border='1')
                    tr
                      td(width="8%")
                      td(width="6%")
                      td(width="12%")
                      td(width="12%")
                      td(width="4%")
                      td(width="8%")
                      td(width="30%")
                      td(word-break="break-all")
                      td(width="6%")
                      each logs in data
                        tr
                          each log in logs
                            td #{log}
          .div4label
            br
            | Solution
            br
            textarea.divcenter#solutionText(rows='4', cols='150') #{solutionText}
script.
  function func(){
    var docURL=document.URL;
    var urlTokens=docURL.split("/");
    var hackType=urlTokens[3];
    var data=document.getElementById(hackType);
    data.style.backgroundColor= '#FC9D9A';
  }